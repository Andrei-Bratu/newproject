import java.sql.*;
public class New {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			Connection con1 = DriverManager.getConnection("jdbc:mysql://localhost:3306/dbverifyServerCertificate=false&useSSL=true");
			Statement st = con1.createStatement();
			ResultSet res = st.executeQuery("select* from player");
			while (res.next()) {
				System.out.println(res.getInt("idplayer") + ", " + res.getString("playercol"));
			}
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}

}
