import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

public class Sql {
	public void register() {
		String url = 
		try {
			final Connection con1 = DriverManager.getConnection(
					url, "root",
					"MySQLpassdrootV:1.0");
			Statement st = con1.createStatement();
			String sql = "INSERT INTO db.player (idplayer, playercol)" + "VALUES (10, 'dsf')";
			st.executeUpdate(sql);
			st.execute("DELETE FROM db.player WHERE ID =" + 1);
			ResultSet res = st.executeQuery("select* from player");
			while (res.next()) {
				System.out.println(res.getInt("idplayer") + ", " + res.getString("playercol"));
			}
			con1.close();
		} catch (final SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
