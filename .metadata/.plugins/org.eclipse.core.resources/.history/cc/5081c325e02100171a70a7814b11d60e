import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

public class Sql {
	public void register() {
		try {
			final Connection con1 = DriverManager.getConnection(
					"jdbc:mysql://localhost:3306/db?verifyServerCertificate=false&useSSL=true", "root",
					"MySQLpassdrootV:1.0");
			Statement st = con1.createStatement();
			String sql = "INSERT INTO player (idplayer, playercol)" + "VALUES (10, 'dsf')";
			st.executeUpdate(sql);
			st.executeUpdate("DELETE FROM player WHERE ID =" + 1);
			ResultSet res = st.executeQuery("select* from player");
			while (res.next()) {
				System.out.println(res.getInt("idplayer") + ", " + res.getString("playercol"));
			}
			con1.close();
		} catch (final SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
